{
    "version": "1.24.2",
    "description": "An open source programming language that makes it easy to build simple, reliable, and efficient software.",
    "homepage": "https://golang.org",
    "license": "BSD-3-Clause",
    "architecture": {
        "64bit": {
            "url": "https://mirror.nju.edu.cn/golang/go1.24.2.windows-amd64.zip",
            "hash": "29c553aabee0743e2ffa3e9fa0cda00ef3b3cc4ff0bc92007f31f80fd69892e1"
        }
    },
    "extract_dir": "go",
    "installer": {
        "script": [
            "$envgopath = \"$env:USERPROFILE\\go\"",
            "if ($env:GOPATH) { $envgopath = $env:GOPATH }",
            "info \"Adding '$envgopath\\bin' to PATH...\"",
            "add_first_in_path \"$envgopath\\bin\" $global"
        ]
    },
    "uninstaller": {
        "script": [
            "$envgopath = \"$env:USERPROFILE\\go\"",
            "if ($env:GOPATH) { $envgopath = $env:GOPATH }",
            "info \"Removing '$envgopath\\bin' from PATH...\"",
            "remove_from_path \"$envgopath\\bin\" $global"
        ]
    },
    "bin": [
        "bin\\go.exe",
        "bin\\gofmt.exe"
    ],
    "env_add_path": ".go\\bin",
    "env_set": {
        "GO111MODULE": "on",
        "GOROOT": "$dir",
        "GOPATH": "$dir\\.go",
        "GOPROXY": "https://mirrors.aliyun.com/goproxy/,https://mirrors.tencent.com/go/,https://mirrors.huaweicloud.com/repository/goproxy/,https://repo.nju.edu.cn/go/,direct"
    },
    "persist": ".go",
    "checkver": {
        "url": "https://golang.org/dl/",
        "regex": "go([\\d.]+)\\.windows-"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://mirror.nju.edu.cn/golang/go$version.windows-amd64.zip"
            }
        },
        "hash": {
            "url": "https://dl.google.com/go/go$version.windows-amd64.zip.sha256"
        }
    }
}
