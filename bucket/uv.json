{
    "version": "0.8.3",
    "description": "An extremely fast Python package installer and resolver, written in Rust.",
    "homepage": "https://docs.astral.sh/uv/",
    "license": "Apache-2.0|MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/astral-sh/uv/releases/download/0.8.3/uv-x86_64-pc-windows-msvc.zip",
            "hash": "4ca84e28b08f48255f95156c5987d61a5e4c51a43372708bc6d84e994eeb7bdb"
        },
        "32bit": {
            "url": "https://github.com/astral-sh/uv/releases/download/0.8.3/uv-i686-pc-windows-msvc.zip",
            "hash": "5d272849a94b7ad36711f336d745e08ed3732042fc51f5c7f28bfc4e95463615"
        },
        "arm64": {
            "url": "https://github.com/astral-sh/uv/releases/download/0.8.3/uv-aarch64-pc-windows-msvc.zip",
            "hash": "6e0692b817c5d6cfddad13ad177e866e36d95e8d32b4a296a49d937fdcda18d3"
        }
    },
    "bin": [
        "uv.exe",
        "uvx.exe"
    ],
    "persist": "data",
    "env_set": {
        "PYTHONUTF8": "1",
        "UV_PYTHON_INSTALL_MIRROR": "https://mirror.nju.edu.cn/github-release/indygreg/python-build-standalone",
        "UV_DEFAULT_INDEX": "https://mirrors.tuna.tsinghua.edu.cn/pypi/web/simple",
        "UV_CACHE_DIR": "$dir\\data\\cache",
        "UV_PYTHON_INSTALL_DIR": "$dir\\data\\python",
        "UV_PYTHON_BIN_DIR": "$dir\\data\\bin",
        "UV_TOOL_DIR": "$dir\\data\\tool",
        "UV_TOOL_BIN_DIR": "$dir\\data\\bin"
    },
    "env_add_path": "data\\bin",
    "checkver": {
        "github": "https://github.com/astral-sh/uv"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/astral-sh/uv/releases/download/$version/uv-x86_64-pc-windows-msvc.zip"
            },
            "32bit": {
                "url": "https://github.com/astral-sh/uv/releases/download/$version/uv-i686-pc-windows-msvc.zip"
            },
            "arm64": {
                "url": "https://github.com/astral-sh/uv/releases/download/$version/uv-aarch64-pc-windows-msvc.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
