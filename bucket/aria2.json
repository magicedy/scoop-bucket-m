{
    "version": "1.37.0-1",
    "description": "Lightweight multi-protocol & multi-source command-line download utility",
    "homepage": "https://aria2.github.io/",
    "license": "GPL-2.0-or-later",
    "architecture": {
        "64bit": {
            "url": "https://mirrors.sdu.edu.cn/github-release/aria2_aria2/release-1.37.0/aria2-1.37.0-win-64bit-build1.zip",
            "hash": "67d015301eef0b612191212d564c5bb0a14b5b9c4796b76454276a4d28d9b288",
            "extract_dir": "aria2-1.37.0-win-64bit-build1"
        }
    },
    "bin": "aria2c.exe",
    "checkver": {
        "url": "https://api.github.com/repos/aria2/aria2/releases/latest",
        "jsonpath": "$..browser_download_url",
        "regex": "download/release-(?:[\\d.]+)/aria2-(?<version>[\\d.]+)-win-64bit-build(?<build>[\\d]+)\\.zip",
        "replace": "${version}-${build}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://mirrors.sdu.edu.cn/github-release/aria2_aria2/release-$matchVersion/aria2-$matchVersion-win-64bit-build$matchBuild.zip",
                "extract_dir": "aria2-$matchVersion-win-64bit-build$matchBuild"
            }
        }
    }
}
