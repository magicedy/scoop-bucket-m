{
    "version": "8.0.4c",
    "description": "Disk benchmark",
    "homepage": "https://osdn.net/projects/crystaldiskmark/",
    "license": "BSD-3-Clause",
    "url": "https://mirror.nju.edu.cn/osdn/crystaldiskmark/77936/CrystalDiskMark8_0_4c.zip",
    "hash": "0513b11acde65e3c26cefaa90edea4896b4fce1d67478af16b23f5a8cf9c7512",
    "pre_install": [
        "'64', '32' | ForEach-Object {",
        "    if (!(Test-Path \"$persist_dir\\DiskMark$_.ini\")) { New-Item \"$dir\\DiskMark$_.ini\" | Out-Null }",
        "}"
    ],
    "architecture": {
        "64bit": {
            "bin": [
                [
                    "DiskMark64.exe",
                    "DiskMark"
                ]
            ],
            "shortcuts": [
                [
                    "DiskMark64.exe",
                    "CrystalDiskMark"
                ]
            ]
        },
        "32bit": {
            "bin": [
                [
                    "DiskMark32.exe",
                    "DiskMark"
                ]
            ],
            "shortcuts": [
                [
                    "DiskMark32.exe",
                    "CrystalDiskMark"
                ]
            ]
        }
    },
    "persist": [
        "DiskMark32.ini",
        "DiskMark64.ini"
    ],
    "checkver": {
        "url": "https://crystalmark.info/en/download/",
        "regex": "CrystalDiskMark\\s+([\\w.]+)\\s"
    },
    "autoupdate": {
        "url": "https://mirror.nju.edu.cn/osdn/crystaldiskmark/$matchRelease/CrystalDiskMark$underscoreVersion.zip"
    }
}
