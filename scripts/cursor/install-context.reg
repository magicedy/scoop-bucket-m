[HKEY_CURRENT_USER\Software\Classes\*\shell\Open with &Cursor]
@="Open with &Cursor"
"Icon"="$cursor"
[HKEY_CURRENT_USER\Software\Classes\*\shell\Open with &Cursor\command]
@="\"$cursor\" \"%1\""

[HKEY_CURRENT_USER\Software\Classes\Directory\shell\Open with &Cursor]
@="Open with &Cursor"
"Icon"="$cursor"
[HKEY_CURRENT_USER\Software\Classes\Directory\shell\Open with &Cursor\command]
@="\"$cursor\" \"%1\""

[HKEY_CURRENT_USER\Software\Classes\Directory\Background\shell\Open with &Cursor]
@="Open with &Cursor"
"Icon"="$cursor"
[HKEY_CURRENT_USER\Software\Classes\Directory\Background\shell\Open with &Cursor\command]
@="\"$cursor\" \"%V\""
